#include <bits/stdc++.h>
using namespace std;
int second_largest(int arr[],int n){
    if (n < 2) {
        cout << "There is no second largest element." << endl;
        return -1;
    }

    
    int largest=arr[0];
    for (int i=0;i<n;i++){
        if (arr[i]>largest){
            largest=arr[i];
        }
    }
    
    int slargest=INT_MIN;
    for (int i=0;i<n;i++){
        if (arr[i]>slargest && arr[i]!=largest){
            slargest=arr[i];
        }
    }
    return slargest;
}



int main(){
    int n;
    cin>>n;
    int arr[n];
    for (int i=0;i<n;i++) cin>>arr[i];
    int x=second_largest(arr,n);
    cout<<x<<endl;
    return 0;
}
